msgid "'W'  (0.0, 0.5)"
msgstr "'W' (0.0, 0.5)"

#: matplotlib.axes._base._AxesBase.set_anchor:22 of
msgid "'C' (0.5, 0.5)"
msgstr "'C' (0.5, 0.5)"

#: matplotlib.axes._base._AxesBase.set_anchor:22 of
msgid "'E'  (1.0, 0.5)"
msgstr "'E' (1.0, 0.5)"

#: matplotlib.axes._base._AxesBase.set_anchor:24 of
msgid "'SW' (0.0, 0.0)"
msgstr "'SW' (0.0, 0.0)"

#: matplotlib.axes._base._AxesBase.set_anchor:24 of
msgid "'S' (0.5, 0.0)"
msgstr "'S' (0.5, 0.0)"

#: matplotlib.axes._base._AxesBase.set_anchor:24 of
msgid "'SE' (1.0, 0.0)"
msgstr "'SE' (1.0, 0.0)"

#: matplotlib.axes._base._AxesBase.set_anchor:35 of
msgid "**share**"
msgstr "**Compartir**"

#: of
msgid "bool, default: False"
msgstr "bool, por defecto: False"

#: matplotlib.axes._base._AxesBase.set_anchor:28 of
msgid "If ``True``, apply the settings to all shared Axes."
msgstr "Si es ``True``, aplique la configuración a todos los ejes compartidos."

