msgid "**ind**"
msgstr "**ind**"

#: of
msgid "list of int"
msgstr "lista de int"

#: matplotlib.lines.VertexSelector.process_selected:8 of
msgid "The indices of the selected vertices."
msgstr "Los índices de los vértices seleccionados."

#: matplotlib.lines.VertexSelector.process_selected:24 of
msgid "**xs, ys**"
msgstr "**xs, ys**"

#: of
msgid "array-like"
msgstr "array-like"

#: matplotlib.lines.VertexSelector.process_selected:11 of
msgid "The coordinates of the selected vertices."
msgstr "Las coordenadas de los vértices seleccionados."

