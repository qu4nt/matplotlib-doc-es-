#: matplotlib.pyplot.clabel:2 of
msgid "Label a contour plot."
msgstr "Etiqueta una parcela de contorno."

#: matplotlib.pyplot.clabel:4 of
msgid "Adds labels to line contours in given `.ContourSet`."
msgstr "Añade etiquetas a los contornos de línea en el `.ContourSet` dado."

#: matplotlib.pyplot.clabel of
msgid "Parameters"
msgstr "Parámetros"

#: matplotlib.pyplot.clabel:9 of
msgid "**CS** : `.ContourSet` instance"
msgstr "**CS** : `.ContourSet` instance"

#: matplotlib.pyplot.clabel:8 of
msgid "ContourSet instance"
msgstr "Instancia ContourSet"

#: matplotlib.pyplot.clabel:9 of
msgid "Line contours to label."
msgstr "Líneas contornos para etiquetar."

#: matplotlib.pyplot.clabel:13 of
msgid "**levels**"
msgstr "**Niveles**"

#: of
msgid "array-like, optional"
msgstr "tipo arreglo, opcional"

#: matplotlib.pyplot.clabel:12 of
msgid "A list of level values, that should be labeled. The list must be a subset of ``CS.levels``. If not given, all levels are labeled."
msgstr "Una lista de valores de nivel, que debe ser etiquetada. La lista debe ser un subconjunto de ``CS.levels``. Si no se proporciona, todos los niveles están etiquetados."

